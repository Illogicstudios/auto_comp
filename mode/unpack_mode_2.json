{
  "name": "Unpack Mode 2",
  "layers": [
    {
      "name": "BG",
      "rule": "^BG$",
      "options": {"color": [55, 55, 150]}
    },
    {
      "name": "FG",
      "rule": "^FG$",
      "options": {"color": [140, 50, 125]}
    },
    {
      "name": "SMOKE",
      "rule": "^SMOKE$",
      "options": {"color": [55, 150, 115]}
    },
    {
      "name": "ATMO",
      "rule": "^ATMO$",
      "options": {"color": [145, 150, 95]}
    }
  ],
  "shuffle": {
    "shuffle_layer": ["BG", "FG", "SMOKE"]
  },
  "merge": {
    "rules": [
      {
        "a": "FG",
        "b": "BG",
        "operation": "over",
        "result": "FG_OVER_BG"
      },
      {
        "a": "SMOKE",
        "b": "FG_OVER_BG",
        "operation": "over",
        "result": "SMOKE_OVER_FG_BG"
      },
      {
        "a": "ATMO",
        "b": "SMOKE_OVER_FG_BG",
        "operation": "plus"
      }
    ]
  }
}
