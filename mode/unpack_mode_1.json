{
  "name": "Unpack Mode 1",
  "layers": [
    {
      "name": "BG",
      "rule": "^BG$",
      "options": {"color": [55, 55, 150]}
    },
    {
      "name": "CHARS",
      "rule": "^CHARS$",
      "options": {"color": [140, 50, 125]}
    },
    {
      "name": "ATHMO",
      "rule": "^ATHMO$",
      "options": {"color": [55, 150, 115]}
    }
  ],
  "shuffle": {
    "shuffle_layer": ["CHARS", "BG"]
  },
  "merge": {
    "rules": [
      {
        "a": "CHARS",
        "b": "BG",
        "operation": "over",
        "result": "CHARS_OVER_BG"
      },
      {
        "a": "ATHMO",
        "b": "CHARS_OVER_BG",
        "operation": "plus"
      }
    ]
  }
}
